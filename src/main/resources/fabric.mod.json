{
  "schemaVersion": 1,
  "id": "creaturecraftmod",
  "version": "${version}",

  "name": "Creature Craft",
  "description": "Creature Craft removes things that we don't like and adds things that might improve the game",
  "authors": [
    "CopperFox0505"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.copperfox.creaturecraftmod.CreatureCraftMod"
    ],
    "client": [
      "net.copperfox.creaturecraftmod.CreatureCraftClient"
    ]
  },
  "mixins": [
    "creaturecraftmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
